{
	"parser": "babel-eslint",
	"extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": [
    "react"
  ],
  "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
      }
  },
  "rules": {
    "no-console": "off",
    "react/prop-types": 0
  },
	"globals": {
    "console": true,
    "document": false,
    "window": false,
    "clearInterval": false,
    "setInterval": false
  }
}